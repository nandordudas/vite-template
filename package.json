{
  "name": "vite-template",
  "type": "module",
  "version": "0.5.1",
  "private": true,
  "packageManager": "pnpm@10.10.0",
  "scripts": {
    "dev": "vite --config .config/vite.ts",
    "build": "vite --config .config/vite.ts build",
    "preview": "vite preview",
    "lint:fix": "pnpm lint --fix",
    "lint": "eslint --cache --config .config/eslint.ts",
    "typecheck": "vue-tsc -p tsconfig.app.json",
    "upgrade": "pnpx taze major -iw && pnpm dedupe",
    "bumpp": "pnpx bumpp"
  },
  "dependencies": {
    "@nuxt/ui-pro": "catalog:prod",
    "pinia": "catalog:prod",
    "pnpm-workspace-yaml": "catalog:prod",
    "vue": "catalog:prod",
    "vue-router": "catalog:prod"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:lint",
    "@types/node": "catalog:types",
    "@vitejs/plugin-vue": "catalog:dev",
    "@vue/tsconfig": "catalog:lint",
    "eslint": "catalog:lint",
    "eslint-plugin-format": "catalog:lint",
    "typescript": "catalog:dev",
    "vite": "catalog:dev",
    "vue-tsc": "catalog:lint"
  }
}
